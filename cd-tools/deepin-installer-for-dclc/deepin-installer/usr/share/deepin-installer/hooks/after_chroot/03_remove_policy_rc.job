# delete policy-rc.d script if it already exist.

RC="/target/usr/sbin/policy-rc.d"

if [ -f "${RC}" ]; then
  msg "policy-rc.d exists.Will remove it..."
  rm "$RC"
else
  msg "policy-rc.d not exists"
fi

rm -f /target/etc/apt/apt.conf.d/00deepin-installer

return 0
